{
  "name": "beautypanel",
  "version": "1.1.6",
  "description": "Starter template for creating Adobe UXP based photoshop plugin.",
  "author": "Adobe Inc",
  "license": "Apache-2.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "rollup -c --watch",
    "start-styles": "sass --watch scss:ccx/styles --no-source-map --load-path=node_modules",
    "build": "npm run build-styles && npm run build-bundle",
    "build-bundle": "NODE_ENV=production rollup -c",
    "build-styles": "sass scss:ccx/styles --no-source-map --load-path=node_modules"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.14.5",
    "@chiogen/rollup-plugin-terser": "^7.1.3",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/photoshop": "^24.3.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "rollup": "^3.19.1",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-import-css": "^3.2.1",
    "sass": "^1.59.3",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.4",
    "@reduxjs/toolkit": "^1.9.3",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "eslint": "^8.36.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-lit": "^1.8.2",
    "i18next": "^19.8.4",
    "path": "^0.12.7",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^8.0.5",
    "redux": "^4.0.5",
    "tslib": "^2.0.3",
    "vue": "^3.2.19"
  }
}
