{
  "name": "beautypanel",
  "version": "1.1.8.alpha-3",
  "description": "Beautypanel plugin for Photoshop",
  "author": "Chiogen <iwashier@gmail.com>",
  "license": "MIT",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "webpack --watch",
    "start-styles": "sass --watch scss:ccx/styles --no-source-map --load-path=node_modules",
    "build": "npm run build-styles && npm run build-bundle",
    "build:production": "npm run build:bundle && npm run build:styles",
    "build:bundle": "NODE_ENV=production webpack",
    "build:styles": "sass scss:ccx/styles --no-source-map --load-path=node_modules"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.14.5",
    "@types/photoshop": "^24.3.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "css-loader": "^6.8.1",
    "sass": "^1.59.3",
    "ts-loader": "^9.4.3",
    "typescript": "^5.0.2",
    "webpack": "^5.84.1",
    "webpack-cli": "^5.1.1"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.4",
    "@reduxjs/toolkit": "^1.9.3",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "eslint": "^8.36.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-lit": "^1.8.2",
    "i18next": "^19.8.4",
    "path": "^0.12.7",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^8.0.5",
    "redux": "^4.0.5",
    "tslib": "^2.0.3"
  }
}
