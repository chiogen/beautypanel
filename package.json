{
  "name": "beautypanel",
  "version": "1.0.0",
  "description": "Starter template for creating Adobe UXP based photoshop plugin.",
  "author": "Adobe Inc",
  "license": "Apache-2.0",
  "main": "index.js",
  "scripts": {
    "start": "rollup -c --watch",
    "start-styles": "sass --watch scss:ccx/styles --no-source-map --load-path=node_modules",
    "build": "npm run build-styles && npm run build-bundle",
    "build-bundle": "NODE_ENV=production rollup -c",
    "build-styles": "sass scss:ccx/styles --no-source-map --load-path=node_modules"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.14.5",
    "@rollup/plugin-alias": "^3.1.5",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-typescript": "^2.1.0",
    "@rollup/pluginutils": "^4.1.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "rollup": "^2.35.1",
    "rollup-plugin-consts": "^1.0.2",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-import-css": "^2.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.32.0",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.4",
    "i18next": "^19.8.4",
    "path": "^0.12.7",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "redux": "^4.0.5",
    "tslib": "^2.0.3",
    "vue": "^3.2.19"
  }
