{
  "manifestVersion": 4,
  "id": "com.chiogen.beautypanel",
  "name": "Beautypanel",
  "version": "1.0.0",
  "main": "index.js",
  "host": [
    {
      "app": "PS",
      "minVersion": "22"
    }
  ],
  "entrypoints": [
    {
      "type": "panel",
      "id": "com.chiogen.beautypanel.panel",
      "minimumSize": {
        "width": 230,
        "height": 200
      },
      "maximumSize": {
        "width": 2000,
        "height": 2000
      },
      "preferredDockedSize": {
        "width": 230,
        "height": 300
      },
      "preferredFloatingSize": {
        "width": 230,
        "height": 300
      },
      "label": {
        "default": "Beautypanel"
      },
      "icons": [
          {"width":23,"height":23,"path":"icons/dark.png","scale":[1,2],"theme":["darkest","dark","medium"]},
          {"width":23,"height":23,"path":"icons/light.png","scale":[1,2],"theme":["lightest","light"]}
      ]
    }
  ],
  "icons": [
    {
      "width": 23,
      "height": 23,
      "path": "icons/dark.png",
      "scale": [
        1,
        2
      ],
      "theme": [
        "darkest",
        "dark",
        "medium"
      ]
    },
    {
      "width": 23,
      "height": 23,
      "path": "icons/light.png",
      "scale": [
        1,
        2
      ],
      "theme": [
        "lightest",
        "light"
      ]
    }
  ]
}